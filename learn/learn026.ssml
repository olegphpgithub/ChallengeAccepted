    They found the fossil skeleton of a mastodon.<break time="1000ms"/>
    <!-- Они нашли окаменевший скелет мастодонта. -->

    Bolster<break time="2000ms"/>
    <!-- поддерживать, подстрекать, буфер, валик, брус, подкладка, шейка -->
